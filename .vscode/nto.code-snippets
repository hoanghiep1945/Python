{
	"Check prime number": {
		"prefix": "nto",
		"body": [
			"import math",
			"",
			"def nt(n):",
			"    if n < 2:",
			"        return False",
			"    for i in range(2, int(math.sqrt(n)) + 1):",
			"        if n % i == 0:",
			"            return False",
			"    return True"
		],
		"description": "Check if a number is prime"
	}
